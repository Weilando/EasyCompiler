none <- main() {
  int x = 42;
  int y = 7;
  int z = 3;

  print(4 == 4);    // true
  print(3 == -3);   // false
  print(x == y);    // false
  print(x == 42);   // true
  println(2 == z);  // false

  print(1 != 0);    // true
  print(2 != -2);   // true
  print(3 != 3);    // false
  print(y != z);    // true
  print(z != 2);    // true
  println(3 != z);  // false

  print(-3 < 3);    // true
  print(y < z);     // false
  print(x < 42);    // false
  println(2 < z);   // true

  print(3 > -3);    // true
  print(z > y);     // false
  print(x > 42);    // false
  println(2 > z);   // false

  print(-3 <= 3);   // true
  print(y <= z);    // false
  print(x <= 42);   // true
  println(2 <= z);  // true

  print(3 >= -3);   // true
  print(z >= y);    // false
  print(x >= 42);   // true
  println(2 >= z);  // false
}
