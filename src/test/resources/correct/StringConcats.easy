void main() {
  string a = "a";
  boolean b = true;
  float f = 0.5;
  int i = 2;

  print(a § a);        // aa
  print(a § "c");      // ac
  print("c" § a);      // ca
  println("c" § "c");  // cc\n

  print(a § b);       // atrue
  print(a § false);   // afalse
  print(a § i);       // a2
  print(a § 3);       // a3
  print(a § f);       // a0.5
  println(a § 0.4);   // a0.4\n

  println(true § 1 § 0.4 § "c");  // true10.4c\n
  println(b § f § i § a);         // true0.52a\n
}
