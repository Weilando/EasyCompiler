none <- main() {
  float f1 = 3;
  float f2 = -2;
  int i = 2;
  println(f1); // 3.0
  println(f2); // -2.0

  println(1 + 2.0); // 3.0
  println(3.5 - 2); // 1.5
  println(2 * 3.2); // 6.4
  println(7 / 2.0); // 3.5

  f1 = 2 + f1;
  f2 = f2 - i;
  println(f1);      // 5.0
  println(f2);      // -4.0
  println(f1 * i);  // 10.0
  println(i / f2);  // -0.5

  // First result is int (14), all other 7 results are float (14.0)
  print(2 + 3 * 4);
  print(2 + 3 * 4.0);
  print(2 + 3.0 * 4);
  print(2 + 3.0 * 4.0);
  print(2.0 + 3 * 4.0);
  print(2.0 + 3.0 * 4);
  print(2.0 + 3 * 4.0);
  println(2.0 + 3.0 * 4.0);
}
